    "lucide-react": "^0.487.0",
    "material-dynamic-colors": "^1.1.2",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "shadcn": "2.4.0-canary.12",
    "stylelint": "^16.19.1",
    "stylelint-no-unsupported-browser-features": "^8.0.4"
  },

  "devDependencies": {
    "@eslint/compat": "^1.2.5",
