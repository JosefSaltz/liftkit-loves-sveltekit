    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-svelte": "^3.0.0",
    "fast-glob": "^3.3.3",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.0",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
